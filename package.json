{        "author" : "ELLIOTTCABLE (http://ell.io/tt)"

,          "name" : "Paws.js"
,       "private" :  true
,       "version" : "5.0.0"
,  "version-name" : "nordic paws"

,     "spec-name" : "Paws' Nucleus 10 (ish.)"

,          "main" : "Library/Paws.js"

,           "bin" : {"paws.js" : "Executables/paws.js"}

,       "scripts" :{
             "build" : "./Scripts/build.sh"
,       "prepublish" : "./Scripts/build.sh"

,             "test" : "./Scripts/test.sh"
,               "ci" : "./Scripts/travis.sh"

,             "repl" : "./Scripts/repl.js"
,            "clean" : "./Scripts/clean.sh"
,   "install-git-hooks" :
                       "./Scripts/install-git-hooks.sh"
}

,     "config" :{
           "dirs" :{"source"     : "Source"
                  , "bin"        : "Executables"
                  , "test"       : "Test"
                  , "rulebook"   : "Test/Rulebook"
                  , "integration": "Test/Integration"
                  , "documentation"
                                 : "Docs"
                  , "coverage"
                                 : "Docs/Coverage" }
,         "mocha" :{"ui"         : "bdd"
                  , "reporter"   : "mocha-fivemat-reporter" }
,           "git" :{"hooks"      : "prepare-commit-msg pre-commit post-merge post-checkout" }
 }

,          "engines" :{"node":"~0.10.7"}
,     "dependencies" :{"coffee-script"          : "^1.9.1"
                     , "uuid"                   : "^3.0.1"
                     , "pegjs"                  : "~0.8.0"

                     , "minimist"               : "^1.2.0"
                     , "mustache"               : "^2.2.0"
                     , "blessed"                : "^0.1.81"
                     , "strip-ansi"             : "^3.0.0"

                     , "optional"               : "^0.1.3"
                     , "pretty-error"           : "^2.0.2"
                     , "bluebird"               : "^3.0.6"
                     , "lodash-compat"          : "^3.10.1"                                        }

, "optionalDependencies":{
                       "kexec"                  : "^3.0.0"                                        }

,  "devDependencies" :{"mocha"                  : "^3.2.0"
                     , "expect.js"              : "^0.3.1"
                     , "sinon"                  : "^1.17.2"
                     , "sinon-expect"           : "^0.3.0"
                     , "mocha-fivemat-reporter" : "^0.0.9"
                     , "mocha-clean"            : "^1.0.0"

                     , "coffee-coverage"        : "^0.7.0"
                     , "istanbul"               : "^0.4.1"

                     , "node-inspector"         : "^0.12.3"
                     , "chokidar-cli"           : "^1.2.0"

                     , "taper"                  : "^0.5.0"
                     , "yamljs"                 : "^0.2.4"                                        }

,   "testling" :{
          "files" : "Test/*.coffee"
   ,       "html" : "Library/TAP.html"
   , "preprocess" : "cake --tests compile:client"
   ,   "browsers" :{"ie":      [ 6, 7, 8, 9, 10 ]
                  , "firefox": [ 3, 3.5, 3.6, 4, 5, 10, 19, "nightly" ]
                  , "chrome":  [ 4, 5, 6, 7, 10, 15, 25, "canary" ]
                  , "safari":  [ 4.0, "5.0.5", 5.1, 6.0 ]
                  , "opera":   [ 10.0, 10.5, 11.0, 11.5, 11.6, 12.0, "next" ]
                  , "iphone":  [ 6.0 ]
                  , "ipad":    [ 6.0 ]
                  , "android-browser": [ 4.2 ]                                                   }}}
